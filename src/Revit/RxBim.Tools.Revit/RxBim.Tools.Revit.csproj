<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

    <PropertyGroup>
        <TargetFramework>net472</TargetFramework>
        <PlatformTarget>x64</PlatformTarget>
        <NoWarn>NU1701</NoWarn>
        <AssemblyName>$(MSBuildProjectName).$(Version)</AssemblyName>
        <UseWPF>true</UseWPF>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="CSharpFunctionalExtensions" Version="2.31.0" />
        <PackageReference Include="BimLab.Revit.Api.2019" Version="1.0.1" />
        <PackageReference Include="RevitTask" Version="3.0.0" />
        <PackageReference Include="RxBim.Di" Version="1.1.0-dev005" />
        <PackageReference Update="JetBrains.Annotations" Version="2022.1.0" />
        <PackageReference Update="StyleCop.Analyzers" Version="1.2.0-beta.435">
          <PrivateAssets>all</PrivateAssets>
          <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
        </PackageReference>
    </ItemGroup>

    <PropertyGroup>
         <Description>RxBim Revit extensions Library</Description>
        <PackageId>RxBim.Tools.Revit</PackageId>
    </PropertyGroup>

    <ItemGroup>
        <AssemblyAttribute Include="System.Windows.Markup.XmlnsPrefix">
             <_Parameter1>https://bimteam.ru/RxBim/revit/abstractions</_Parameter1>
            <_Parameter2>ra</_Parameter2>
        </AssemblyAttribute>
        <AssemblyAttribute Include="System.Windows.Markup.XmlnsDefinition">
             <_Parameter1>https://bimteam.ru/RxBim/revit/abstractions</_Parameter1>
             <_Parameter2>RxBim.Tools.Revit.Abstractions</_Parameter2>
        </AssemblyAttribute>
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\..\Core\RxBim.Tools\RxBim.Tools.csproj" />
      <ProjectReference Include="..\..\Core\RxBim.Tools.TableBuilder\RxBim.Tools.TableBuilder.csproj" />
    </ItemGroup>

</Project>